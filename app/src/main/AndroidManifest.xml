<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tawk.experiment.application">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launch"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.tawk.experiment.application.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tawk.experiment.application.MainActivity" />
        <activity android:name="com.tawk.experiment.application.InformationActivity" />
        <activity android:name="com.tawk.experiment.application.MyProfile"></activity>
    </application>

</manifest>